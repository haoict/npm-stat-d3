{
  "version": "1.1.0",
  "author": {
    "name": "Phuc Nguyen Hoang",
    "email": "phucnguyenhoang@gmail.com"
  },
  "description": "Npm package download in graph",
  "license": "MIT",
  "engines": {
    "node": ">=8.11.1"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/kidjp1985/npm-stat-d3"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development node src/server",
    "start": "cross-env NODE_ENV=production node build/server",
    "prebuild": "rimraf build",
    "build": "yarn build:client && yarn build:server",
    "build:client": "cross-env NODE_ENV=production webpack -p --progress --config=config/webpack.prod.js",
    "build:server": "babel -a -u 1 src/server/**/**/* -d build",
    "lint": "eslint -c .eslintrc src/**/*.js",
    "lint:fix": "eslint --fix -c .eslintrc src/**/*.js",
    "test": "jest",
    "test:dev": "jest --watch --coverage",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "resetMocks": true,
    "resetModules": true,
    "verbose": true,
    "browser": true,
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "setupFiles": [
      "<rootDir>/tests/setup.js"
    ],
    "moduleNameMapper": {
      "^helper$": "<rootDir>/tests/helper.js",
      "^mocks(.*)$": "<rootDir>/tests/mocks$1",
      "^utils(.*)$": "<rootDir>/src/client/utils$1",
      "^configs(.*)$": "<rootDir>/src/client/configs$1",
      "^components(.*)$": "<rootDir>/src/client/components$1",
      "^reducers(.*)$": "<rootDir>/src/client/reducers$1",
      "^selectors(.*)$": "<rootDir>/src/client/selectors$1",
      "^containers(.*)$": "<rootDir>/src/client/containers$1",
      "^stores(.*)$": "<rootDir>/src/client/stores$1",
      "^actions(.*)$": "<rootDir>/src/client/actions$1",
      "^graphqlData(.*)$": "<rootDir>/src/client/graphqlData$1"
    },
    "testMatch": [
      "<rootDir>/tests/**/*.test.js"
    ]
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.3",
    "babel-jest": "^23.4.0",
    "babel-loader": "7.1.4",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-watch": "^2.0.7",
    "copyfiles": "2.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.0.1",
    "eslint-config-prettier": "2.3.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-babel": "4.1.1",
    "eslint-plugin-import": "2.6.1",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-prettier": "2.1.2",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "html-loader": "0.5.5",
    "html-webpack-harddisk-plugin": "0.2.0",
    "html-webpack-plugin": "3.2.0",
    "jest": "^23.4.1",
    "lodash-webpack-plugin": "^0.11.5",
    "prettier": "1.12.1",
    "react-hot-loader": "^4.3.4",
    "redux-logger": "3.0.6",
    "redux-mock-store": "^1.5.3",
    "rimraf": "2.6.2",
    "uglifyjs-webpack-plugin": "1.2.5",
    "wait-for-expect": "^1.0.0",
    "webpack": "^4.16.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "3.1.3",
    "webpack-hot-middleware": "2.22.1",
    "webpack-merge": "4.1.2",
    "whatwg-fetch": "2.0.4"
  },
  "dependencies": {
    "@nivo/line": "^0.42.1",
    "apollo-cache-inmemory": "^1.2.6",
    "apollo-client": "^2.3.7",
    "apollo-link": "^1.2.2",
    "apollo-link-http": "^1.5.4",
    "axios": "^0.18.1",
    "compression": "1.7.2",
    "cross-env": "5.1.5",
    "date-fns": "^1.29.0",
    "express": "4.16.3",
    "express-graphql": "^0.6.12",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "lodash": "^4.17.19",
    "polished": "^1.9.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-apollo": "^2.1.9",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "recompose": "^0.27.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1",
    "styled-components": "^3.3.3",
    "styled-media-query": "^2.0.2",
    "winston": "2.4.2"
  }
}
